      <loc>https://www.fbadigest.com/<%= post.slug %></loc>
      <lastmod><%= post.updated_at.iso8601 %></lastmod>
      <changefreq>weekly</changefreq>
      <priority>0.80</priority>
    </url>
  <% end %>

